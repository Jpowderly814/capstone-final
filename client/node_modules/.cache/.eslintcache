urn (\n      <svg\n        className=\"w-6 h-6\"\n        fill={fill}\n        stroke=\"currentColor\"\n        viewBox=\"0 0 24 24\"\n        xmlns=\"http://www.w3.org/2000/svg\"\n      >\n        <path\n          strokeLinecap=\"round\"\n          strokeLinejoin=\"round\"\n          strokeWidth=\"2\"\n          d=\"M11.049 2.927c.3-.921 1.603-.921 1.902 0l1.519 4.674a1 1 0 00.95.69h4.915c.969 0 1.371 1.24.588 1.81l-3.976 2.888a1 1 0 00-.363 1.118l1.518 4.674c.3.922-.755 1.688-1.538 1.118l-3.976-2.888a1 1 0 00-1.176 0l-3.976 2.888c-.783.57-1.838-.197-1.538-1.118l1.518-4.674a1 1 0 00-.363-1.118l-3.976-2.888c-.784-.57-.38-1.81.588-1.81h4.914a1 1 0 00.951-.69l1.519-4.674z\"\n        ></path>\n      </svg>\n    );\n  }\n\n  return (\n    <div className=\"box flex\">\n      {[1, 2, 3, 4, 5].map((index) => {\n        return <RatingIcon index={index} rating={average} />;\n      })}\n    </div>\n  );\n};\n\nexport default AverageRating;\n","/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/Domain/Entities/Types/User.js",[],"/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/index.js",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":2,"column":17,"nodeType":"108","messageId":"109","endLine":2,"endColumn":25},{"ruleId":"106","severity":1,"message":"110","line":16,"column":22,"nodeType":"108","messageId":"109","endLine":16,"endColumn":35},{"ruleId":"106","severity":1,"message":"111","line":15,"column":10,"nodeType":"108","messageId":"109","endLine":15,"endColumn":18},{"ruleId":"106","severity":1,"message":"112","line":1,"column":8,"nodeType":"108","messageId":"109","endLine":1,"endColumn":13},{"ruleId":"106","severity":1,"message":"113","line":4,"column":8,"nodeType":"108","messageId":"109","endLine":4,"endColumn":12},"no-native-reassign",["114"],"no-negated-in-lhs",["115"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'setIsLoggedIn'