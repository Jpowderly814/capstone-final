    localStorage.setItem('expiresIn', response.data.token.expires);\n        localStorage.setItem('user', response.data.result[0].id);\n        console.log(localStorage.getItem('user'));\n\n        //setLoginStatus(response.data[0].RowDataPacket.username);\n        setLoggedIn(true);\n      }\n    });\n\n    setUsername('');\n    setPassword('');\n  };\n\n  const logout = () => {\n    Axios.post('http://localhost:3001/logout', {}).then((response) => {\n      if (response.data.message) {\n        setLoggedIn(true);\n\n        setError({\n          title: 'Invalid',\n          message: response.data.message,\n        });\n        return;\n      } else {\n        console.log();\n        localStorage.setItem('token', null);\n        localStorage.setItem('user', null);\n        console.log(localStorage.getItem('token'));\n        console.log(localStorage.getItem('user'));\n\n        //setLoginStatus(response.data[0].RowDataPacket.username);\n        setLoggedIn(true);\n      }\n    });\n    setLoggedIn(false);\n    localStorage.setItem('token', null);\n    localStorage.setItem('user', null);\n    setUsername('');\n    setPassword('');\n  };\n\n  const errorHandler = () => {\n    setError(null);\n  };\n\n  return (\n    <div className=\"login-register-container\">\n      {error && (\n        <ErrorModal\n          title={error.title}\n          message={error.message}\n          onConfirm={errorHandler}\n        />\n      )}\n      <div className=\"text-center\">\n        <h2 className=\"login-register-header\">Sign in</h2>\n      </div>\n\n      <div className=\"login-register-form\">\n        {localStorage.getItem('token') === 'null' && (\n          <div className=\"form-group row\">\n            <label className=\"col-sm-3 col-form-label\">Username</label>\n            <div className=\"col-sm-9\">\n              <input\n                type=\"text\"\n                value={username}\n                onChange={(e) => {\n                  setUsername(e.target.value);\n                }}\n              />\n            </div>\n          </div>\n        )}\n        {localStorage.getItem('token') === 'null' && (\n          <div className=\"form-group row\">\n            <label className=\"col-sm-3 col-form-label\">Password</label>\n            <div className=\"col-sm-9\">\n              <input\n                type=\"password\"\n                value={password}\n                onChange={(e) => {\n                  setPassword(e.target.value);\n                }}\n              />\n            </div>\n          </div>\n        )}\n\n        {localStorage.getItem('token') !== 'null' && (\n          <button className=\"login-btn\" onClick={logout}>\n            logout\n          </button>\n        )}\n        {localStorage.getItem('token') === 'null' && (\n          <button className=\"login-btn\" onClick={login}>\n            login\n          </button>\n        )}\n      </div>\n      <div>\n        {!isRegistering && (\n          <button className=\"login-btn\" onClick={startRegistrationHandler}>\n            register\n          </button>\n        )}\n\n        {isRegistering && <Register onCancel={stopRegistrationHandler} />}\n      </div>\n    </div>\n  );\n}\n\nexport default Login;\n","/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/components/Auth/Register.js",[],"/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/components/Music/Connect.js",[],"/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/components/Music/Dashboard.js",[],"/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/components/Music/Player.js",[],"/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/components/Music/PlaylistSearchResult.js",[],"/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/components/Music/useAuth.js",[],"/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/components/Music/A.js",[],"/Users/powderlj/Desktop/Apprenticeship/capstone-final/client/src/c